{
  "name": "react-webpack-template",
  "version": "0.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "start": "webpack-dev-server --history-api-fallback -d --hot --inline --port 3000 --host 0.0.0.0",
    "build": "set NODE_ENV=production && webpack --progress -p --config ./webpack.prod.js",
    "lint": "eslint src/ test/",
    "lintfix": "eslint --fix src/ test/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "",
  "license": "BSD-2-Clause",
  "dependencies": {
    "clean-webpack-plugin": "^0.1.15",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.26.1",
    "dotenv-webpack": "^1.3.2",
    "es6-promise": "^4.0.5",
    "html-webpack-plugin": "^2.26.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.16.6",
    "mqtt": "^2.3.0",
    "react": "^15.4.2",
    "react-chartjs-2": "^2.0.0",
    "react-dom": "^15.4.2",
    "react-grid-layout": "^0.14.3",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.1",
    "react-responsive": "^1.2.5",
    "react-router": "^3.0.1",
    "react-virtualized": "^8.9.0",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.11",
    "redux-logger": "^2.7.4",
    "redux-persist-immutable": "^4.2.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.13.1"
  },
  "devDependencies": {
    "eslint-loader": "^1.6.1",
    "eslint-plugin-immutablejs": "^0.1.3",
    "babel-eslint": "^7.2.1",
    "babel-preset-stage-0": "^6.16.0",
    "@kadira/storybook": "^2.35.3",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "getstorybook": "^1.7.0",
    "storybook-addon-material-ui": "^0.7.4",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.16.2"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "immutablejs"
    ],
    "rules": {
      "brace-style": 0,
      "immutablejs/no-native-map-set": "error",
      "import/no-extraneous-dependencies": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "linebreak-style": 0,
      "new-cap": 0,
      "newline-per-chained-call": 1,
      "no-mixed-operators": 0,
      "no-param-reassign": 1,
      "radix": 1,
      "react/forbid-prop-types": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/jsx-no-bind": [
        "error"
      ],
      "react/prefer-stateless-function": 0,
      "strict": 0
    },
    "env": {
      "browser": true
    }
  }
}
